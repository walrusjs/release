{
  "npmClient": "yarn",
  "version": "1.0.1",
  "useWorkspaces": true,
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "npmClient": "npm",
      "verifyAccess": false,
      "ignoreChanges": [
        "*.md",
        "**/test/**"
      ],
      "message": "chore(release): publish"
    }
  }
}
